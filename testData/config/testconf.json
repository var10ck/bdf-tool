{
  "writeFormat": "parquet",
  "outputDir": "testData/output",
  "outputFileName": "output.parquet",
  "fields": [
    {
      "name": "id",
      "fType": "uuid(41)"
    },
    {
      "name": "username",
      "fType": "username"
    },
    {
      "name": "phone",
      "fType": "phone_number"
    },
    {
      "name": "email",
      "fType": "email"
    },
    {
      "name": "description",
      "fType": "paragraphs(2)"
    },
    {
      "name": "date",
      "fType": "date(21.01.2000)(31.12.2022)"
    }
  ],
  "rowNum": 10000,
  "batchSize": 1000,
  "oneFile": true,
  "writeOptions": {
    "header": "true"
  }
}